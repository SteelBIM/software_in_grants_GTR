<gtr:projectOverview xmlns:gtr="http://gtr.rcuk.ac.uk/api"><gtr:projectComposition><gtr:collaborations/><gtr:leadResearchOrganisation url="http://gtr.rcuk.ac.uk:80/organisation/DF259B19-FD17-4588-BFEF-AD08F1193451"><gtr:id>DF259B19-FD17-4588-BFEF-AD08F1193451</gtr:id><gtr:name>De Montfort University</gtr:name><gtr:department>Institute of Energy and Sustainable Dev</gtr:department><gtr:address><gtr:line1>The Gateway</gtr:line1><gtr:line4>Leicester</gtr:line4><gtr:line5>Leicestershire</gtr:line5><gtr:postCode>LE1 9BH</gtr:postCode><gtr:region>East Midlands</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:typeInd>RO</gtr:typeInd></gtr:leadResearchOrganisation><gtr:organisationRoles><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/DF259B19-FD17-4588-BFEF-AD08F1193451"><gtr:id>DF259B19-FD17-4588-BFEF-AD08F1193451</gtr:id><gtr:name>De Montfort University</gtr:name><gtr:address><gtr:line1>The Gateway</gtr:line1><gtr:line4>Leicester</gtr:line4><gtr:line5>Leicestershire</gtr:line5><gtr:postCode>LE1 9BH</gtr:postCode><gtr:region>East Midlands</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>LEAD_RO</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/13E7192F-C3F9-42E7-8D0D-AF0B0B7BFE4C"><gtr:id>13E7192F-C3F9-42E7-8D0D-AF0B0B7BFE4C</gtr:id><gtr:name>London Borough of Merton</gtr:name><gtr:address><gtr:line1>Merton Civic Centre</gtr:line1><gtr:line2>London Road</gtr:line2><gtr:postCode>SM4 5DX</gtr:postCode><gtr:region>Unknown</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>PROJECT_PARTNER</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/9280E691-A9ED-4041-9553-23CF8A835185"><gtr:id>9280E691-A9ED-4041-9553-23CF8A835185</gtr:id><gtr:name>The National Energy Foundation</gtr:name><gtr:address><gtr:line1>Davy Avenue</gtr:line1><gtr:line2>Knowlhill</gtr:line2><gtr:line4>Milton Keynes</gtr:line4><gtr:postCode>MK5 8NG</gtr:postCode><gtr:region>South East</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>PROJECT_PARTNER</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/2C24269F-B709-4127-A6E6-84F9687CE16A"><gtr:id>2C24269F-B709-4127-A6E6-84F9687CE16A</gtr:id><gtr:name>JMP Consultanting</gtr:name><gtr:address><gtr:line1>Minerva House</gtr:line1><gtr:line2>East Parade</gtr:line2><gtr:line4>Leeds</gtr:line4><gtr:postCode>LS1 5PS</gtr:postCode><gtr:region>Yorkshire and The Humber</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>PROJECT_PARTNER</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/16A2AC44-FDDA-4753-A927-26136DDCDA6D"><gtr:id>16A2AC44-FDDA-4753-A927-26136DDCDA6D</gtr:id><gtr:name>Polytechnic University of Turin</gtr:name><gtr:address><gtr:line1>Corso Duca degli Abruzzi 24</gtr:line1><gtr:postCode>24-10129</gtr:postCode><gtr:region>Outside UK</gtr:region><gtr:country>Italy</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>PROJECT_PARTNER</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/23ADCB37-9F0F-45DC-AE62-2F174F2EE269"><gtr:id>23ADCB37-9F0F-45DC-AE62-2F174F2EE269</gtr:id><gtr:name>Leicester City Council</gtr:name><gtr:address><gtr:line1>New Walk Centre</gtr:line1><gtr:line2>Welford Place</gtr:line2><gtr:line4>Leicester</gtr:line4><gtr:postCode>LE1 6ZG</gtr:postCode><gtr:region>East Midlands</gtr:region><gtr:country>United Kingdom</gtr:country></gtr:address><gtr:roles><gtr:role><gtr:name>PROJECT_PARTNER</gtr:name></gtr:role></gtr:roles></gtr:organisationRole><gtr:organisationRole url="http://gtr.rcuk.ac.uk:80/organisation/AC87F8BC-3044-409A-BCD0-8E29